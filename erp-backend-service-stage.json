{
  "containerDefinitions": [
    {
      "name": "erp-backend-service-stage",
      "image": "704098412757.dkr.ecr.ap-south-1.amazonaws.com/stage/erp-web-app",
      "cpu": 0,
      "portMappings": [
        {
          "name": "erp-backend-service-5000-tcp",
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "erp-stage-cluster",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    },
    {
      "name": "aws-otel-collector",
      "image": "public.ecr.aws/aws-observability/aws-otel-collector:v0.46.0",
      "cpu": 0,
      "portMappings": [],
      "essential": true,
      "command": [
        "--config=/etc/ecs/ecs-cloudwatch.yaml"
      ],
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "erp-stage-cluster",
          "mode": "non-blocking",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "family": "erp-backend-service-stage",
  "executionRoleArn": "arn:aws:iam::704098412757:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "2048",
  "memory": "4096",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}
